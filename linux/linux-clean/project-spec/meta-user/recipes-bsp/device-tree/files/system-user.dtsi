/include/ "system-conf.dtsi"
/ {
	model = "snickerdoodle Black";
	compatible = "krtkl,snickerdoodle-black", "krtkl,snickerdoodle", "xlnx,zynq-7000";

	chosen {
		bootargs = "console=ttyPS0,115200n8 earlyprintk uio_pdrv_genirq.of_id=generic-uio";
		stdout-path = &uart0;
	};

	aliases {
		serial0 = &uart0;
		spi0 = &qspi;
		spi1 = &spi1;
		mmc0 = &sdhci0;
		mmc1 = &sdhci1;
	};

	wlan_en_reg: fixed-regulator-wlen {
		compatible = "regulator-fixed";
		regulator-name = "wlan-en-regulator";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		gpio = <&gpio0 9 4>;
		startup-delay-us = <70000>;
		enable-active-high;
	};
};

&uart0 {
	status = "okay";
};

&sdhci0 {
	status = "okay";
};

&sdhci1 {
	status = "okay";
	vmmc-supply = <&wlan_en_reg>;
	bus-width = <4>;
	cap-power-off-card;
	keep-power-in-suspend;
	#address-cells = <1>;
	#size-cells = <0>;

	wlcore: wlcore@0 {
		reg = <2>;
		interrupt-parent = <&gpio0>;
		interrupts = <0 4>;
		compatible = "wlcore", "ti,wl1837";
	};
};

&spi1 {
    status = "okay";
    num-cs = <1>;
    is-decoded-cs = <0>;
    #address-cells = <1>;
    #size-cells = <0>;

    dev@0 {
        compatible = "linux,spidev";
        reg = <0>;
        spi-max-frequency = <1000000>;
    };
};

&qspi {
	status = "okay";
	is-dual = <0>;
	num-cs = <1>;
	xlnx,qspi-mode = <1>;

	flash@0 {
		#address-cells = <1>;
		#size-cells =<1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;
		m25p,fast-read;

		partition@bootloader {
			label = "bootloader";
			reg = <0x0 0xe0000>;
		};
		partition@env {
			label = "env";
			reg = <0xe0000 0x20000>;
		};
		partition@linux {
			label = "linux";
			reg = <0x100000 0x500000>;
		};
		partition@devicetree {
			label = "devicetree";
			reg = <0x600000 0x620000>;
		};
		partition@rootfs {
			label = "rootfs";
			reg = <0x620000 0x9e0000>;
		};
	};
};


// Generated pl.dtsi from xsa file at path below
// linux/linux-clean/components/plnx_workspace/device-tree/device-tree/pl.dtsi

&amba_pl {

	/delete-node/ led_driver_top@40000000;

	led_driver_ctrl@40000000 {
		clock-names = "S_AXIL_ACLK";
		clocks = <&clkc 15>;
		compatible = "generic-uio", "xlnx,led-driver-top-1.0";
		reg = <0x40000000 0x2000>;
		xlnx,axil-addr-width = <0x5>;
		xlnx,axil-data-width = <0x20>;
	};

	led_driver_data@40010000 {
		clock-names = "S_AXIF_ACLK";
		clocks = <&clkc 15>;
		compatible = "generic-uio", "xlnx,led-driver-top-1.0";
		reg = <0x40010000 0x10000>;
		xlnx,s-axif-addr-width = <0x10>;
		xlnx,s-axif-data-width = <0x20>;
		xlnx,s-axif-id-width = <0x2>;
	};
};